generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
}

model Destinations {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  slug      String
  location  String
  thumbnail String
}