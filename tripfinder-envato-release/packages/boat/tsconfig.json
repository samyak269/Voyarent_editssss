{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "C:\\Users\\NAHID\\Desktop\\tripfinder\\packages\\boat\\.next/types/**/*.ts",
    "/Users/idris/RedQTeam/RedQApp/tripfinder/packages/boat/.next/types/**/*.ts",
    "C:\\Users\\KL\\Desktop\\tripfinder\\packages\\boat\\.next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
